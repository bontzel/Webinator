<div class="container-fluid">
	<div class="row">
		<div class="col-md-1">
			<div class="thumbnail">
				<img src=<%= @profile.avatar %> alt="avatar image" width=100 height=100>
			</div>
		</div>
	</div>

	<div class="row">
		<ul>
			<li> First name: <%= @profile.first_name %></li>
